{
    "name": "command-runner",
    "displayName": "Command runner",
    "description": "",
    "version": "0.0.4",
    "icon": "images/icon.png",
    "repository": {
        "url": "https://github.com/Quilljou/command-runner"
    },
    "publisher": "QuanZhou",
    "engines": {
        "vscode": "^1.35.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:command-runner.excute"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [{
            "command": "command-runner.excute",
            "title": "Run Command..."
        }],
        "menus": {
            "explorer/context": [{
                "command": "command-runner.excute"
            }],
            "editor/title/context": [{
                "command": "command-runner.excute"
            }]
        },
        "keybindings":[
            {
                "command": "command-runner.excute",
                "key": "cmd+shift+c"
            }
        ],
        "configuration": {
            "type": "object",
            "title": "Command Runner Configuration",
            "properties": {
                "command-runner.commands": {
                    "type": "object",
                    "description": "Set custom command",
                    "scope": "resource",
                    "default": {
                        "ls": "ls",
                        "pwd": "pwd"
                    }
                }
            }
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.21",
        "tslint": "^5.8.0",
        "@types/node": "^8.10.25",
        "@types/mocha": "^2.2.42"
    }
}